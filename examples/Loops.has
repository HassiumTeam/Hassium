func main () {
    # While loop counting from 1 to 10
    x = 1;
    while (x <= 10)
        println(x++);
    
    #For loop counting from 1 to 10
    for (x = 1; x <= 10; x++)
        println(x);
        
    #Do while loop counting from 1 to 10
    x = 1;
    do {
        println(x++);
    } while (x <= 10);
    
    # Foreach loop counting from 1 to 10
    foreach (x in range(1 , 10))
        println(x);
}
