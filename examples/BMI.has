# You can define a funcion with func <funcName>

func calculateBMI(height, weight) {
	return weight / (height * height);
}

func main() {
	print("How tall are you? ");
	# Convert class has functions for turning strings into numbers

	height = Convert.toDouble(input());
	print("How much do you weigh? ");
	weight = Convert.toDouble(input());

	println(calculateBMI(height, weight));
}
