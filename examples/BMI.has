# You define functions with func <funcName>

func calcbmi (height, weight) {
	return weight / (height * height);
}

func main () {
	print("How tall are you? ");
	height = input ().tofloat ();
	
	print("How much to you weigh? ");
	weight = input().tofloat ();

	println("BMI: " + calcbmi(height, weight));
}
