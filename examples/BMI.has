# You define functions with func <funcName>

func calculateBMI (height, weight) {
	return weight / (height * height);
}

func main () {
	print("How tall are you? ");
	height = input().toDouble();
	
	print("Hoe much to you weight? ");
	weight = input().toDouble();

	println("BMI: " + calculateBMI(height, weight));
}
