use Text;

func main () {
	MyClass.one ();
	MyClass.two ();
	StringBuilder.thisDidNotExistBefore ();
}

class MyClass {
	func one () {
		println ("Hello from one");
	}
}

extend MyClass {
	func two () {
		println ("Hello from two");
	}
}

extend StringBuilder {
	func thisDidNotExistBefore () {
		println ("Why hey! I am an addition to StringBuilder!");
	}
}
