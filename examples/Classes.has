class Dog {
	func new(name) {
		this.Name = name;
	}

	func showName(this) {
		println(this.Name);
	}

	func bark(this) {
		println("Woof!");
	}
}

class BigDog : Dog {
	func new() {
	}

	func growl(this) {
		println("GRRRR");
	}
}

func main() {
	fiddo = new Dog("Fiddo");
	fiddo.showName();
	fiddo.bark();
	bigDog = new BigDog();
	bigDog.bark();
	bigDog.growl();
}
