class ArrayList { 
    func new () {
        this.Value = [];
    }
    
    func add (this, n) {
        this.Value = this.Value.resize(2);
        this.Value[this.Value.length - 1] = n;
    }
}

func main () {
    ar = new ArrayList();
    ar.add(3);
    ar.add(4);
    ar.add(5);
    println(ar.Value);
}
