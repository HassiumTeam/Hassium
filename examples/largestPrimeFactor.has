func lpf (x) {
	factor = 0;
	if (x % 2 == 0) {
		factor = 2;
		while (x % 2 == 0)
			x /= 2;
	}
	if (x % 3 == 0) {
		factor = 3;
		while (x % 3 == 0)
			x /= 3;
	}
	factor = 5;
	while (x > factor) {
		while (x % factor == 0)
			x /= factor;
		factor += 2;
		if (x == factor)
			break;
		while (x % factor == 0)
			x /= factor
		factor += 4;
	}
	return factor;
}

func main () {
		println (lpf (1337));
}
